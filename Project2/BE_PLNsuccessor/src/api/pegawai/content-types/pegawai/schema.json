    },
    "ID_GRADE": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::grade.grade",
      "inversedBy": "ID_PEGAWAI"
    },
    "ID_JABATAN": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::jabatan.jabatan",
      "inversedBy": "ID_PEGAWAI"
    },
    "ID_PENDIDIKAN": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pendidilkan.pendidilkan",
      "inversedBy": "ID_PEGAWAI"
    },
    "ID_UNIT": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::unit.unit",
      "inversedBy": "ID_PEGAWAI"
    }
  }
